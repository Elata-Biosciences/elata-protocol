{
  "filter_paths": [
    "lib/",
    "test/",
    "out/",
    "src_deprecated/",
    "test_deprecated/"
  ],
  "exclude_dependencies": true,
  "exclude_optimization": false,
  "exclude_informational": true,
  "exclude_low": true,
  "exclude_medium": false,
  "exclude_high": false,
  "solc_remaps": [
    "@openzeppelin/=lib/openzeppelin-contracts/",
    "forge-std/=lib/forge-std/src/"
  ],
  "detectors_to_exclude": [
    "pragma",
    "solc-version",
    "low-level-calls",
    "naming-convention",
    "unused-return",
    "missing-zero-check",
    "calls-loop"
  ],
  "disable_color": true
}
